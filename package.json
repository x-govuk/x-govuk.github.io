{
  "name": "x-govuk",
  "version": "1.0.0",
  "description": "A blog about collaboration across the UK government",
  "homepage": "https://x-govuk.github.io",
  "bugs": {
    "url": "https://github.com/x-govuk/x-govuk.github.io/issues"
  },
